{
  "extends": "@zodiac/typescript-config",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "paths": {
      "@/utils": ["./src/utils/index.ts"],
      "@/types": ["./src/types.ts"],
      "@/const": ["./src/const.ts"],
      "@/execution-routes": ["./src/panel/execution-routes/index.ts"],
      "@/state": ["./src/panel/state/index.tsx"],
      "@/providers": ["./src/panel/providers/index.ts"],
      "@/providers-ui": ["./src/panel/providers-ui/index.ts"],
      "@/port-handling": ["./src/panel/port-handling/index.ts"],
      "@/inject-bridge": ["./src/inject/bridge/index.ts"],
      "@/e2e-utils": ["./e2e/utils/index.ts"],
      "@/test-utils": ["./test-utils/index.ts"],
      "@/messages": ["./src/messages.ts"],
      "@/transaction-translation": [
        "./src/panel/transactionTranslations/index.ts"
      ],
      "@/safe": ["./src/panel/integrations/safe/index.ts"],
      "@/zodiac": ["./src/panel/integrations/zodiac/index.ts"]
    }
  },
  "include": [
    "./src/**/*",
    "./e2e/**/*",
    "./test-utils/**/*",
    "./vitest.setup.mts"
  ]
}
