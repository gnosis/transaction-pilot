{
  "compilerOptions": {
    "strict": true,
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "paths": {
      "@/transfer": ["./app/transfer"],
      "@/components": ["./app/components"],
      "@/clients": ["./app/clients"],
      "@/config": ["./app/config"]
    }
  },
  "include": ["./app/**/*"],
  "exclude": ["node_modules"]
}
